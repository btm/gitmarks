       <link rel="stylesheet" type="text/css" media="screen" href="http://www.streetsblog.org/wp-content/plugins/occomment/oc-styles.css" />
<script type="text/javascript">
/* <![CDATA[ */
var OpenCore = {
    /* Comment Page Initialization.  (i.e. Logged in User query and response handling,
     * as well as javascriptification of the page.
     * */
    init: function() {
        OpenCore.unhide('oc_login');
        var oc_login = $j('oc_login');
        if (oc_login) {
            oc_login.attr('href',OpenCore.oc_url + 'login?came_from_anchor=comment-form');
            oc_login.click(OpenCore.uselogin);
        }
        $j('#anchor-vacuum-login').removeClass('hide');
        // Add a script that calls our prepareform() callback.
        OpenCore.addscript(OpenCore.oc_url + 'user.js');
        // Prettify inline password fields
        if( document.createElement && document.childNodes ) {
            var passFields = $j('input[type=password].oc-js-compactPassword');
            passFields.each(function(el) {
                var passElem = document.createElement('input');
                passElem.setAttribute('type','text');
                passElem.setAttribute('value','password');
                // We need both cases of this attribute to support all browsers...
                passElem.setAttribute('tabindex', this.dom.tabIndex);
                passElem.setAttribute('tabIndex', this.dom.tabIndex);
                this.dom.parentNode.insertBefore(passElem, this.dom);
                passElem = $j(passElem);
                var passField = $j(this);
                passField.hide();
                passElem.show();
                passElem.focus( function() {
                    passElem.hide();
                    passField.show();
                    passField.focus();
                });
                passField.blur( function() {
                    if (passField.dom.value != '') {
                        return;
                    }
                    passElem.show();
                    passField.hide();
                });
            });
        }
        this.addMouseOverBehavior();
    },
    addMouseOverBehavior: function() {
        function _addOverBehavior(selection) {
          selection.mouseover(function(e, el, o) { $j(this).addClass('over'); });
          selection.mouseout(function(e, el, o) { $j(this).removeClass('over'); });
        }
        _addOverBehavior($j('ul#uninav > li.initiative'));
        _addOverBehavior($j('.oc-dropdown-list li.lsi-dropdown-container'));
    },
    wp_url: 'http://www.streetsblog.org',
    oc_url: 'http://www.livablestreets.com/',
    wp_blogname: 'Streetsblog New York City',
    memberInfo: {loggedin: false},
    uselogin: function(e) {
        if (OpenCore.memberInfo.loggedin) {
            e.preventDefault();
            OpenCore.prepareform(OpenCore.memberInfo);
        }
    },
    prepareform: function(memberInfo) {
        if (memberInfo.loggedin)
        {
            OpenCore.memberInfo = memberInfo;

            var commentform = $j('#comment-form');
            if (commentform) {
                commentform.submit(OpenCore.relaysubmit);
                // Populate and hide the classic login form, and present our interface
                $j('#author_info').hide();

                // Sice the opencore username is optional, we'll use the id if they
                // don't have a username
                $j('#author').val(memberInfo.name.length ? memberInfo.name : memberInfo.id);

                $j('#email').val(memberInfo.email);

                // In fact, for now, we force the profile URL.
                $j('#url').val(memberInfo.memberurl);

var commentbox = '<p class="oc-login">\
Currently logged in using LivableStreets ID \
<a href="'+OpenCore.memberInfo.profileurl +'">'+ OpenCore.memberInfo.id + '</a>\
 | <a id="oc-link-logout" href="'+OpenCore.oc_url+'">Log out</a>\
 | <a id="oc-link-anonymous" href="">Post anonymously</a>\
</p>\
<p class="oc-login">\
<input id="oc-log-comment" type="checkbox" checked="checked" />\
Show this comment on my profile\
</p>';

                $j('#openplans_info').append(commentbox);
                $j('#oc-link-logout').click(OpenCore.logout);
                $j('#oc-link-anonymous').click(OpenCore.removeinterface);
            }
                // Clean out the logout/login box before using it
            $j('#anchor-vacuum-login > *').remove();
            $j('#anchor-vacuum-login').append(OpenCore.memberInfo.topnav);
            //OC.breatheLife(Ext.get('anchor-vacuum-login').dom);
        }
    },
    logout: function(e) {
        OpenCore.removeinterface(e);
        OpenCore.memberInfo = {loggedin: false};
        // In order to get the cookies necessary to actually logout, we have to add this as a script tag, even if it's not script
        OpenCore.addscript(OpenCore.oc_url + 'logout.js');
        e.preventDefault();
    },
    /* Restore and clear the classic login form. */
    removeinterface: function(e) {
        $j("#comment-form").unbind("submit", OpenCore.relaysubmit);
        $j('#author').val('');
        $j('#email').val('');
        $j('#url').val('');
        $j('#author_info').show();
        $j('#openplans_info > *').remove();
        $j('#anchor-vacuum-login > *').remove();
        e.preventDefault();
    },
    /* Simple shortcut for adding a script tag to the page. */
    addscript: function(scriptSource) {
        if( document.createElement && document.childNodes ) {
            var scriptElem = document.createElement('script');
            scriptElem.setAttribute('type','text/javascript');
            scriptElem.setAttribute('src',scriptSource);
            document.getElementsByTagName('body')[0].appendChild(scriptElem);
        }
    },
    /* Handler for comment form submission. */
    relaysubmit: function(e) {
        var logCheck = $j('#oc-log-comment');
        var mcsp = $j('#mcspvalue');
        if (mcsp) {
            var label = $j('label[for=mcspvalue]')[0];
            if (label) {
                var match = label.innerHTML.match(/Spam protection: Sum of\s+([0-9()+-/*\s]+)\s*\?/);
                if (match) {
                    var equation = match[1];
                    var result = eval(equation);
                    mcsp.val(result);
                }
            }
        }
        var submitButton = $j('#submit');
        if (submitButton && submitButton.get(0)) {
            submitButton.attr('disabled','true');
        }
        if ((logCheck == undefined) || (!logCheck.attr('checked'))) {
            return true;
        }
        OpenCore.unhide('oc-progress-spinner');
        e.preventDefault();
        // Post comment to WP first, because we care about the comment ID.
        $j.ajax({
            type:'POST',
            url:$j('#comment-form').attr('action'),
            data: $j('#comment-form').serialize(),
            success: OpenCore.wpcomment_onsuccess,
            error: OpenCore.wpcomment_onfailure
            });
        return true;
    },
    wpcomment_onsuccess: function(msg) {
        // Get the URL to the new comment and include that in our form.
        var commentids = msg.match(/<li[^>]*id="comment-(\d+)"/g);
        if (! commentids) {
            OpenCore.wpcomment_onfailure(msg);
            return;
        }
        var commentid = commentids.slice(-1)[0].match(/<li[^>]*id="comment-(\d+)"/)[1];
        OpenCore.commenturl = location.href.split('#')[0] + '#comment-' + commentid;
        var commentform = $j('#comment-form');
        var commentElem = document.createElement('input');
        commentElem.setAttribute('type', 'hidden');
        commentElem.setAttribute('name', 'commenturl');
        commentElem.setAttribute('value', OpenCore.commenturl);
        commentform.append(commentElem);
        var blognameElem = document.createElement('input');
        blognameElem.setAttribute('type', 'hidden');
        blognameElem.setAttribute('name', 'blog_name');
        blognameElem.setAttribute('value', OpenCore.wp_blogname);
        commentform.append(blognameElem);

        // Since we're acting cross-domain, which means we have to pass parameters via the URL, hence
        // we have an implicit limit.  Since we only use excerpts anyway, we'll trim off anything more
        // than about 1k and pass that over.
        var commentbox = $j('#comment');
        if (commentbox && commentbox.get(0) && commentbox.val() && commentbox.val().length > 1024) {
            commentbox.val(commentbox.val().substr(0, 1024));
        }
        // Add a script tag that posts the trackback to openplans,
        // but persevere eventually if the script source fails to load.
        OpenCore._submittimer = setTimeout(function() {
            OpenCore.submitstatus(false,  "Timed out while sending comment to openplans.");
            var submitButton = $j('#submit', false);
            if (submitButton && submitButton.get(0)) {
                submitButton.attr('disabled','false');
            }
        }, 30000);
        OpenCore.addscript(OpenCore.memberInfo.memberurl + '/trackback?' + commentform.serialize());
        return true;
    },
    wpcomment_onfailure: function(msg) {
        if (msg.responseText.indexOf('You are posting comments too quickly') != -1 ){
            alert('You are posting comments too quickly. Slow down.');
        } else {
            alert('Posting comment to wordpress failed. Try again later.');
        }
        OpenCore.hide('oc-progress-spinner');
    },
    /* Callback that runs after the trackback is posted to openplans. */
    submitstatus: function(status, msg) {
        clearTimeout(OpenCore._submittimer);
        if (! status) {
            if (msg == undefined) {
                msg = "unspecified error";
            }
            alert(msg);
        };
        OpenCore.hide('oc-progress-spinner');
        location.href = OpenCore.commenturl;
        // Since that's the same url with a different anchor,
        // we have to explicitly tell the browser to reload.
        location.reload();
    },
    hide: function(name) {
        var hide = $j(name);
        if (hide) hide.addClass('oc-hidden');
    },
    unhide: function(name) {
        var unhide = $j(name);
        if (unhide) unhide.removeClass('oc-hidden');
    }
}
$j = jQuery.noConflict();
$j(document).ready(function() {

  /* Short and Sweet */
  OpenCore.init();

});
/* ]]> */
</script>

