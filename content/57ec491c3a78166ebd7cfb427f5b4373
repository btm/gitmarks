  <script type="text/javascript">
    function showGroup(group) {
      $("*.row").each(function () {
        $(this).hide();
      });
      $("#nodata").hide();
      if ($("*.row." + group).size() == 0){
        $("#nodata").fadeIn();
      } else {
        $("*.row." + group).each(function () {
          $(this).fadeIn();
        });
      }
    }
    $(document).ready(function (){
      $(window).load(function (){
        $("#grouplist").show();
      });
      $("*.group").click(
        function () {
          showGroup($(this).text());
        });
    });
  </script>

