<script type="text/javascript" defer="defer">

function attach(){
        var form = document.getElementsByName('getForecast')[0];
        if(!form){
                setTimeout(attach, 100);
                return;
        }

	form.search.maxLength = 120;
        
        form.onsubmit = function(e){
                getLocationResults(this.search, document.getElementById('showLocations'), document.getElementById('progressIcon'));
		return false;
        }
		viewport = new GLatLngBounds(new GLatLng(46.1, -126.25), new GLatLng(49.08, -120.46));
	
}

// from http://stackoverflow.com/questions/778983/detecting-cause-of-ies-operation-aborted-issue
function delayExecutionForIE(fnCallback) {
      if ( typeof document.all == "object" && 
           (document.readyState != "loaded" 
           && document.readyState != "complete") 
         ) {
            try {
                  //If IE is used, use the trick by Diego Perini
                  //http://javascript.nwbox.com/IEContentLoaded/
                  document.documentElement.doScroll("left");
                  fnCallback.call(this);
            } catch(error) {
                  setTimeout(function(){delayExecutionForIE(fnCallback);}, 200);
            }
      } else {
            fnCallback.call(this);
      }
}

//setTimeout(attach, 1); // to (hopefully) avoid IE's "Operation aborted" error.
delayExecutionForIE(function(){
