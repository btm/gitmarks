                <div class="maybe-sprite tid-11">
                        <span class="no-sprite" style="display: none">background: url(//a.fsdn.com/sd/topics/topicms.gif); width:75px; height:55px;</span>
                        <a style="width:75px; height:55px; display: block; text-indent:-99999px;" href="//slashdot.org/index2.pl?fhfilter=microsoft" onclick="return addfhfilter('microsoft');" class="microsoft"  >microsoft</a>
                </div>
                
        


