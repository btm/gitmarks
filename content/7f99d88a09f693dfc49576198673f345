<script>
		
		function goSub()
		{
			var val;
			val = '0';
			for(i=0; i < document.form.poll_ans.length; i++)
			{
					if(document.form.poll_ans[i].checked == true)
					{
						//alert(1);	
						//document.form.submit();
						val = '1';
						break;
					}
			}	
			
			if (val == '1')
			{
				document.form.submit();
			}
			else
			{
				alert('Please select from one of the options in order to submit your poll.');	
				//return false;
			}
			
		}
 	</script>
